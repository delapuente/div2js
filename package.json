{
  "name": "div2js",
  "version": "0.0.1",
  "description": "A DIV2 transpiler for the modern web",
  "scripts": {
    "postinstall": "grunt hookmeup",
    "test": "concurrently \"npm run grammar\" \"karma start ./tests/karma.conf.js\"",
    "grammar": "watch-run -p 'src/grammar.y' npm run parser",
    "parser": "jison src/grammar.y --module-type amd --outfile src/div2lang.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/delapuente/div2js"
  },
  "keywords": [
    "div2",
    "game",
    "engine",
    "transpiler",
    "jison"
  ],
